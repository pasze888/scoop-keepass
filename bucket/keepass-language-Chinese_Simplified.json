{
    "version": "2.58",
    "description": "A translations for KeePass 2.x",
    "homepage": "https://keepass.info/translations.html",
    "license": "Unknown",
    "depends": "extras/keepass",
    "url": "https://sourceforge.net/projects/keepass/files/Translations%202.x/2.58/KeePass-2.58-Chinese_Simplified.zip",
    "hash": "sha1:d88f630e20fc8aaae6cf869e00755b1f3420e266",
    "installer": {
        "script": "Copy-Item \"$dir\\Chinese_Simplified.lngx\" \"$(appdir keepass $global)\\current\\Languages\" -Recurse"
    },
    "uninstaller": {
        "script": "Remove-Item \"$(appdir keepass $global)\\current\\Languages\\Chinese_Simplified.lngx\" -Recurse"
    },
    "checkver": {
        "url": "https://sourceforge.net/projects/keepass/rss",
        "regex": "/Translations 2\\.x/([\\d.]+)/KeePass-[\\d.]+-Chinese_Simplified\\.zip"
    },
    "autoupdate": {
        "url": "https://sourceforge.net/projects/keepass/files/Translations%202.x/$version/KeePass-$version-Chinese_Simplified.zip"
    }
}
